#
# PRE: if
#
&Tmp-String-0	:= "AbCdE"

&Tmp-String-0	:= "%{toupper:%{Tmp-String-0}}"
&Tmp-String-1	:= "%{toupper:AAbbCCdd}"
&Tmp-String-2	:= "%{toupper:%{Tmp-String-9}}"

if (&Tmp-String-0 != "ABCDE") {
	test_fail
}

if (&Tmp-String-1 != "AABBCCDD") {
	test_fail
}

if (&Tmp-String-2) {
	test_fail
}

success
