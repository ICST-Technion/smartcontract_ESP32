#
# PRE: if
#
&Tmp-String-0	:= "AbCdE"

&Tmp-String-0	:= "%{tolower:%{Tmp-String-0}}"

&Tmp-String-1	:= "%{tolower:AAbbCCdd}"

#
#  Doesn't exist
#
&Tmp-String-2	:= "%{tolower:%{Tmp-String-9}}"


if (&Tmp-String-0 != "abcde") {
	test_fail
}

if (&Tmp-String-1 != "aabbccdd") {
	test_fail
}

if (&Tmp-String-2) {
	test_fail
}

success
